/**app.wxss**/
@import './common/font/iconfont.wxss';
@import './dist/style/weui.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.clearfix:after{
  content:".";
  display:block;
  height:0;
  clear:both;/*清除浮动*/
  visibility:hidden;
}
.clearfix{zoom:1;/*为IE6，7的兼容性设置*/}
