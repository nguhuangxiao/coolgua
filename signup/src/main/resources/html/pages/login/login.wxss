@import '../../common/font/iconfont.wxss';
@import '../../common/css/base.wxss';
@import "../../wxParse/wxParse.wxss";
/**index.wxss**/
.su-banner{
  width: 100%;
}
.su-banner>image{
  display: block;
  width: 100%;
  height: auto;
}
.item{
  border-bottom:1rpx solid #ddd;
}
.su-title{
  font-weight: bold;
  font-size: 36rpx;
  text-align: center;
}
.su-ctem{
  display: flex;
  align-items: center;
  direction: row;
}
.ctem-top{margin-top:20rpx;}
.su-ctem .icon{
  font-size: 50rpx;
}
.su-ctem .icon-share{margin-left:20rpx;}