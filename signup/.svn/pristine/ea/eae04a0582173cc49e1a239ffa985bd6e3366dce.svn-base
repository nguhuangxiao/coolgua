.sidebar{
  width: 230px;
  float: left;
  background: #fff;
  border: 1px solid #eeeeee;
  .title{
    border-bottom: 1px solid #eeeeee;
    padding: 10px 15px
  }
  .sidebar-main{
    padding: 15px;
    .listbox{
      margin: 15px 0;
      font-size: 12px;
      p{
        margin-bottom: 7px;
        border-bottom: 1px dashed #ddd;
        padding-bottom: 5px;
      }
    }
  }
  select{
    width: 196px;
    height: 27px;
  }
}
