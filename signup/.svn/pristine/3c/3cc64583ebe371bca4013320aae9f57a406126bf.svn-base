.react-dialog-person{
    .anticon-close-circle-o{
      font-size: 28px;
      float: right;
      margin-top: -14px;
      margin-right: -14px;
      color: #aba6a6;
    }
    width: 393px;
    height: 314px;
    //background: rgba(253, 252, 251, 0.9);
    background: url('../../common/image/dialog_bg.jpg') no-repeat;
    position: absolute;
    top: 0px;
    left: 0px;
    color: #373839;
    box-shadow: 0px 0px 20px #c7c6c6;
    z-index: 22;
    .innner{padding:10px;}
    .base{display: flex;}
    .information{
    	padding:10px;
    	padding-left:20px;
      display: flex;
      flex-direction: column;
      justify-content: center;
      .detail{
          font-size: 12px;
          line-height: 20px;
          margin-top: 10px;
      }
      p{
        margin: 0px;
        line-height: 23px;
        font-size: 16px;
        height: 23px;
      }
      .detail>span{
      	display:block;
      }
    }
    .base .logo{
    	width:126px;
    	height:158px;
    	display:block;
      box-shadow: 0px 0px 2px #e0e0e0;
    }
    .static{
    	  background: rgba(255, 255, 255, 0.6);
        box-shadow: 0px 0px 3px #e8e7e7;
        font-size: 12px;
        padding: 10px;
        text-align: center;
        margin-top: 10px;
        ul{
        	margin: 0px;
          padding: 0px;
          list-style: none;
      	  line-height: 25px;
          label{
            margin: 0 10px;
            span{
              font-size: 18px;
            }
          }
          .moreli{
            >label:first-child{margin-left:0px;}
            >label:last-child{margin-right:0px;}
          }
        }
    }
    .ticket{
        display: flex;
        padding: 10px 20px;
        font-size: 14px;
        align-items: center;
        justify-content: space-between;
        margin-top: 8px;
        .yqbtn{
            border: 2px solid #fdbc3f;
            background: #fdf3e1;
            padding: 5px;
            font-size: 12px;
        }
    }
}
